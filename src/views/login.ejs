<%- include('header') %>

    <h1>Welcome Stranger</h1>
    <h2>Be our guest...</h2>
    
    <!--Log In Start-->
    <div>
      <form id="userLoginForm" action="/login/loginUserDDBB" method="POST">
        <label form="userLoginForm" for="email">
          Email:
          <input id="email" name="email" type="text" placeholder="Email..." required>
        </label>
        <label form="userLoginForm" for="password">
          Password:          
          <input id="password" name="password" type="password" placeholder="Password..." required>
        </label>
        <button id="send-button" form="userLoginForm" type="submit">Log In</button>
      </form>
    </div>
    <!--Log In End-->

<%- include('footer') %>